    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-chai-friendly": "^1.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "execa": "^9.3.0",
    "flatbuffers": "^24.3.25",
    "globals": "^15.8.0",
    "renamer": "^5.0.1",
    "typescript-eslint": "^7.16.1"
