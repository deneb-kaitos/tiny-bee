{
  "name": "@deneb-kaitos/tiny-bee-serializers-serde",
  "private": true,
  "version": "0.0.0",
  "description": "SerDe",
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": "22.5.1",
    "pnpm": "9.6.0"
  },
  "engineStrict": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "ESLINT_USE_FLAT_CONFIG=true ./node_modules/.bin/eslint '**/*.mjs'",
    "build:schema": "cd ../fbs && ./.bin/compile.mjs",
    "specs": "pnpm run lint && pnpm run build:schema && ./node_modules/@deneb-kaitos/tiny-bee-specs/bin/run-specs.mjs",
    "cover": "pnpm run lint && ./node_modules/@deneb-kaitos/tiny-bee-specs/bin/run-cover.mjs"
  },
  "author": "Dmitry N. Medvedev <dmitry.medvedev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@deneb-kaitos/tiny-bee-fbs": "workspace:^",
    "flatbuffers": "catalog:web"
  },
  "devDependencies": {
    "@deneb-kaitos/tiny-bee-specs": "workspace:^",
    "c8": "catalog:specs",
    "chai": "catalog:specs",
    "eslint": "catalog:web",
    "eslint-config-airbnb-base": "catalog:web",
    "eslint-plugin-chai-friendly": "catalog:web",
    "eslint-plugin-import": "catalog:web",
    "eslint-plugin-node": "catalog:web",
    "monocart-coverage-reports": "catalog:specs"
  },
  "c8": {
    "check-coverage": true,
    "lines": 95,
    "functions": 95,
    "branches": 95
  }
}
